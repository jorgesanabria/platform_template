script/source = "extends \"res://addons/platform_actors/platform_actor.gd\"

func wander():
	$timer.start()
	yield($timer, \"timeout\")
	set_direction(direction * -1)
	walk()
	yield($timer, \"timeout\")
	stop()
	$timer.wait_time = 1.0
	yield($timer, \"timeout\")
	wander()
	$timer.wait_time = 2.0

func _on_visibility_notifier_2d_screen_exited():
	print(\"stop\")
	stop()

func _on_visibility_notifier_2d_screen_entered():
	print(\"wander\")
	wander()

func _ready():
	set_direction(1)"

