script/source = "extends Node2D

onready var objective = $enemies.get_child_count()

func _ready():
	for c in $enemies.get_children():
		c.connect(\"tree_exited\", self, \"_on_enemy_tree_exited\")
		
func _on_goal_area_entered(area):
	print(\"player\")
	if !area.is_in_group(\"player\"):
		return
	if objective > 0:
		return
	else:
		$goal/shape/sprite.texture = load(\"res://levels/tilemap/red/flag_raised.png\")
		trigger_animation(\"win\")
	
func _on_enemy_tree_exited():
	objective -= 1
	objective = max(objective, 0)
	
func trigger_animation(animation):
	get_tree().paused = true
	$interface/animator.play(animation)
	yield($interface/animator, \"animation_finished\")
	
	$interface/control/button.show()
	$interface/control/button.grab_focus()
	yield($interface/control/button, \"button_up\")
	get_tree().paused = false
	get_tree().reload_current_scene()"

